pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                echo 'checkout step'
            }
        }
		
        stage('Build') {
            steps {
                
                    echo 'Build step'
                
            }
       	   }
		   
       
	   stage('Deploy'){
	       steps {
		   echo 'deploy step'
	   }
	   }
	   
	   
    }
}
